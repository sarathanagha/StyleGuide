  <Target Name="BuildSvgs" BeforeTargets="PreComputeCompileTypeScript" Inputs="@(ResourceSvgs)" Outputs="$(SvgTS)">
    <Message Text="Executing gulp build-svgs for project: $(MSBuildProjectName)" />
    <Exec Command="$(GulpBuildCommand) build-svgs" ConsoleToMSBuild="true">
      <Output TaskParameter="ConsoleOutput" PropertyName="BuildOutput" />
      <Output TaskParameter="ExitCode" PropertyName="BuildExitCode" />
    </Exec>
    <Message Text="Gulp build-svg Exit Code: $(BuildExitCode)" />
    <Error Condition="$(BuildExitCode) != 0" Text="Gulp build-svgs build failed!" />
  </Target>
  <Target Name="CompileResx" Inputs="$(ResourcesDir)ClientResources.resx" Outputs="$(GeneratedDir)ClientResources.txt">
    <GenerateResource Sources="$(ResourcesDir)ClientResources.resx" OutputResources="$(GeneratedDir)ClientResources.txt">
    </GenerateResource>
  </Target>
  <!-- TODO iannight: use T4 templates or some other native templating solution -->
  <Target Name="BuildResources" DependsOnTargets="CompileResx" BeforeTargets="PreComputeCompileTypeScript" Inputs="$(GeneratedDir)ClientResources.txt" Outputs="$(ClientResourcesTS)">
    <Message Text="Executing gulp build-resources for project: $(MSBuildProjectName)" />
    <Exec Command="$(GulpBuildCommand) build-resources-msbuild" ConsoleToMSBuild="true">
      <Output TaskParameter="ConsoleOutput" PropertyName="BuildOutput" />
      <Output TaskParameter="ExitCode" PropertyName="BuildExitCode" />
    </Exec>
    <Message Text="Gulp build-resources Exit Code: $(BuildExitCode)" />
    <Error Condition="$(BuildExitCode) != 0" Text="Gulp build-resources-msbuild failed!" />
  </Target>
  <Target Name="BuildFramework" BeforeTargets="PreComputeCompileTypeScript" Inputs="@(FrameworkModules)" Outputs="$(FrameworkTS)">
    <Message Text="Executing gulp build-framework for project: $(MSBuildProjectName)" />
    <Exec Command="$(GulpBuildCommand) build-framework" ConsoleToMSBuild="true">
      <Output TaskParameter="ConsoleOutput" PropertyName="BuildOutput" />
      <Output TaskParameter="ExitCode" PropertyName="BuildExitCode" />
    </Exec>
    <Message Text="Gulp build-framework Exit Code: $(BuildExitCode)" />
    <Error Condition="$(BuildExitCode) != 0" Text="Gulp build-framework failed!" />
  </Target>
  <Target Name="CreateGenerated" BeforeTargets="BuildSvgs;BuildResources;CompileResx;BuildFramework">
    <MakeDir Directories="$(GeneratedDir)" />
  </Target>
  <!-- After Build Tasks -->
  <Target Name="BuildSass" AfterTargets="Build" Inputs="@(SASSFiles)" Outputs="@(SASSFiles->'$(OutputDir)%(RecursiveDir)%(FileName).scss')">
    <Message Text="Executing gulp build-sass for project: $(MSBuildProjectName)" />
    <Exec Command="$(GulpBuildCommand) build-sass-msbuild" ConsoleToMSBuild="true">
      <Output TaskParameter="ConsoleOutput" PropertyName="BuildOutput" />
      <Output TaskParameter="ExitCode" PropertyName="BuildExitCode" />
    </Exec>
    <Message Text="Gulp build-sass Exit Code: $(BuildExitCode)" />
    <Error Condition="$(BuildExitCode) != 0" Text="Gulp build-sass failed!" />
    <Copy SourceFiles="@(SassFiles)" DestinationFolder="$(OutputDir)%(RecursiveDir)" SkipUnchangedFiles="true" />
  </Target>
  <Target Name="CopyStaticFiles" AfterTargets="Build">
    <!-- these wildcards need to happen inside of the target so we don't exclude the generated files -->
    <ItemGroup>
      <StaticFiles Include="$(SrcDir)**\*.html">
        <InProject>false</InProject>
      </StaticFiles>
      <StaticFiles Include="$(SrcDir)**\*.svg">
        <InProject>false</InProject>
      </StaticFiles>
      <StaticFiles Include="$(SrcDir)**\*.css">
        <InProject>false</InProject>
      </StaticFiles>
      <StaticFiles Include="$(SrcDir)**\*.css.map">
        <InProject>false</InProject>
      </StaticFiles>
      <StaticFiles Include="$(SrcDir)**\*.js">
        <InProject>false</InProject>
      </StaticFiles>
      <StaticFiles Include="$(SrcDir)**\*.ts">
        <InProject>false</InProject>
      </StaticFiles>
      <StaticFiles Include="$(SrcDir)**\*.png">
        <InProject>false</InProject>
      </StaticFiles>
    </ItemGroup>
    <Copy SourceFiles="@(StaticFiles)" DestinationFolder="$(OutputDir)%(RecursiveDir)" SkipUnchangedFiles="true" />
    <Copy SourceFiles="$(SrcDir)\package.json" DestinationFolder="$(OutputDir)" SkipUnchangedFiles="true" />
  </Target>
  <!-- Clean -->
  <Target Name="CleanGenerated" AfterTargets="Clean">
    <RemoveDir Directories="$(OutputDir);$(GeneratedDir)" ContinueOnError="false" />
  </Target>
  <!-- Linting and other gulp tests -->
  <Target Name="RunTests" AfterTargets="Build" Condition="'$(Configuration)' == 'Release'">
    <Message Text="Executing gulp run-tests for project: $(MSBuildProjectName)" />
    <Exec Command="$(GulpBuildCommand) production" ConsoleToMSBuild="true">
      <Output TaskParameter="ConsoleOutput" PropertyName="BuildOutput" />
      <Output TaskParameter="ExitCode" PropertyName="BuildExitCode" />
    </Exec>
    <Message Text="Gulp run-tests Exit Code: $(BuildExitCode)" />
    <Error Condition="$(BuildExitCode) != 0" Text="Gulp run-tests failed!" />
  </Target>
