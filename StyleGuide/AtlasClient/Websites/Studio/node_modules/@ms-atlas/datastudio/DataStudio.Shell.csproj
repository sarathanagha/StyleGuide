    <GulpBuildCommand>$(GulpCmd) --gulpfile gulpfile-msbuild.js default</GulpBuildCommand>
    <GulpCleanCommand>$(GulpCmd) --gulpfile gulpfile-msbuild.js clean</GulpCleanCommand>
  </PropertyGroup>
  <Target Name="BuildResx" BeforeTargets="PrepareForBuild">
    <Message Text="Building resx.js" />
    <Exec Command="$(NodeExe) translate-resx.js ../resources nls/resx.js 1" ConsoleToMSBuild="true">
      <Output TaskParameter="ConsoleOutput" PropertyName="BuildOutput" />
      <Output TaskParameter="ExitCode" PropertyName="BuildExitCode" />
    </Exec>
    <Message Text="node.exe exit code: $(BuildExitCode)" />
    <Error Condition="$(BuildExitCode) != 0" Text="Building resx.js failed, please see above output!" />
  </Target>
  <Target Name="GulpBuildTasks" AfterTargets="Build">
    <Message Text="Executing the gulp for project: $(MSBuildProjectName)" />
    <Exec Command="$(GulpBuildCommand)" ConsoleToMSBuild="true">
      <Output TaskParameter="ConsoleOutput" PropertyName="BuildOutput" />
      <Output TaskParameter="ExitCode" PropertyName="BuildExitCode" />
    </Exec>
    <Message Text="Gulp Exit Code: $(BuildExitCode)" />
    <Error Condition="$(BuildExitCode) != 0" Text="Gulp build failed." />
  </Target>
  <Target Name="AfterClean">
    <!-- Delete task cannot be used because the files are readonly -->
    <Message Text="Executing gulp clean for project: $(MSBuildProjectName)" />
    <Exec Command="$(GulpCleanCommand)" ConsoleToMSBuild="true">
      <Output TaskParameter="ConsoleOutput" PropertyName="BuildOutput" />
      <Output TaskParameter="ExitCode" PropertyName="BuildExitCode" />
    </Exec>
    <Message Text="Gulp Exit Code: $(BuildExitCode)" />
    <Error Condition="$(BuildExitCode) != 0" Text="Gulp clean failed." />
  </Target>
