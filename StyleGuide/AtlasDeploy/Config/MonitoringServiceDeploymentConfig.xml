      <Setting Name="Microsoft.Diagnostics.EnableMdsTracing">True</Setting>
      <Setting Name="Microsoft.Diagnostics.LogLevel">All</Setting>

      <Setting Name="MonitoringXStoreAccounts">MdsAtlasEastUSTest#false#atlasmdseastustest##http://table.core.windows.net#http://queue.core.windows.net#http://blob.core.windows.net#CURRENT_USER\MY</Setting>
      <Setting Name="MAConfigFileLocation">..\MdsConfig\MDSAtlas_Test.xml</Setting>

      <Setting Name="Microsoft.DataStudio.Services.Tenant">72f988bf-86f1-41af-91ab-2d7cd011db47</Setting>
      <Setting Name="Microsoft.DataStudio.Services.Audience">https://management.core.windows.net/</Setting>
      <Setting Name="Microsoft.DataStudio.Services.LoginEndpoint">https://login.windows.net/</Setting>
      <Setting Name="Microsoft.DataStudio.Services.FederationMetadataVersion">2007-06</Setting>
    </Settings>
    <ScaleUnit Type="ScaleUnit" Name="SU01">
      <HostedService Type="Microsoft.DataStudio.Deployment" Name="$(DeploymentName)">
        <Service>
          <Role Name="UxMonitoring.WebRole"></Role>
        </Service>
      </HostedService>
    </ScaleUnit>
  </Config>

