      <Setting Name="SecretStore">https://test.secretstore.core.azure-test.net/CertSvc.svc</Setting>
      <Setting Name="Microsoft.TableStorage.ConnectionString">DefaultEndpointsProtocol=https;AccountName=$(@AppStorage.Account);AccountKey=$(@AppStorage.ActiveKey)</Setting>

      <Setting Name="DeploymentName">atlasmonitoringtest</Setting>

      <Setting Name="PackagePath">Publish\MonitoringService\Package.Monitoring.cspkg</Setting>
      
